{
  "name": "hovanka-site",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev --port 3000 --host",
    "format": "prettier --write .",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
    "lint": "prettier --check . && eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@headlessui/tailwindcss": "^0.2.1",
    "@inlang/paraglide-sveltekit": "^0.15.5",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.0.0",
    "dotenv": "^16.4.7",
    "svelte-headlessui": "^0.0.46",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@inlang/paraglide-js": "1.11.8",
    "@sveltejs/adapter-cloudflare": "^5.0.1",
    "@sveltejs/enhanced-img": "^0.4.4",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@zapal/dx": "^1.2.0",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.19.3",
    "svelte-check": "^4.1.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  }
}
